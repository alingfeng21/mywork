@charset "utf-8";
/* CSS Document */
body{background-color:#1366ce; margin:0; overflow-x:hidden;}
img{ display:block; border:none;}
ul{ margin:0; padding:0; list-style:none;}
a{ text-decoration:none;}
table{ border-collapse:collapse; border-spacing:0; width:800px; margin:0 auto; padding:4px; font-size:12px; margin-top:10px;}
h4{padding:0; margin:10px 20px 20px 10px;}
li{ margin:0; padding:0;}
/*top*/
.big_ba{ width:1000px; background-color:red; margin:auto;}
.top{ width:100%; margin:auto; margin-top:0;}
.top_ba{ width:1400px; margin:0 auto;  background-image:url(images/top_nav.png); background-position:center center; background-repeat:no-repeat;overflow:hidden;}
.top_nav{ float:right; height:30px; width:300px; margin-right:130px;}
.top_nav a{font-size:12px; line-height:30px; padding:5px; background-image:url(images/xian_1.png); background-position:right center; background-repeat:no-repeat; color:white;}
.top_nav .xian{ background-image:none;}
.top_banner img{ width:100%; margin:0 auto;}/*让图片拉伸（背景图片可以平铺）充满屏幕*/

.nav{ width:1000px; margin:auto; background-color:#1a60b6;}
.nav_list{ height:40px;  margin:0 auto; width:900px;}
.nav_list li{ float:left; width:150px; text-align:center; line-height:40px; color:#F00;}
.nav_list li a{font-size:14px; color:white; display:block;}
.nav_list li img{ display:none;}
.nav_list li .current{display:block;}
.nav_list li:hover a{ color:#F00; display:block;}
/*banner*/
.banner{ width:1000px; margin:0 auto;}

/*main_1*/
.xinxi{width:1000px; margin:0 auto; overflow:hidden;  margin-top:8px; margin-bottom:10px;}
.xinxi_1{ float:left; width:280px; height:260px; margin-top:30px;margin-left:10px;}
.xinxi_1 li{ padding:8px;}
.xinxi_1 li a{ display:block; font-size:12px;} 


.show_box{ background-color:#ebebeb; padding:1px;float:left;width:420px; height:270px; margin-top:10px; }
.photo_box{ background-color:white; padding:1px; position:relative; overflow:hidden;}
.button_box { position:absolute; right:0; bottom:0; overflow:hidden;}
.button_box a{ float:left;width:20px;font-size:12px; line-height:20px; text-align:center; background-color:#999; margin:1px;opacity:0.5; cursor: pointer; }
.button_box{ margin-right:5px; margin-bottom:4px; overflow:hidden;}
.text_box{ line-height:33px; text-align:center; font-size:12px; position:absolute; left:20px; bottom:0; color:white}
.button_box .current{ background-color: #CCC; color: white;}
.photo_box img{ float:left;}


.xinxi_2{ float:right; width:280px; height:260px; margin-top:30px;}
.xinxi_2 li{ padding:8px;}
.xinxi_2 li a{ display:block; font-size:12px;}
/*main_2*/
.yj_box{ width:1000px; margin:0 auto; overflow:hidden;}
.title{ width:1000px; margin:0 auto; background-color:#1a60b6; height:40px;overflow:hidden; }
.title a{display:block; color:white; line-height:40px;text-align:center;}
.yj{overflow:hidden; width:980px; margin:0 auto; margin-top:10px;}
.yj_1{ float:left; width:225px; margin:0 10px;}
.yj_1 p{ font-size:12px; text-align:center; margin:8px; padding:0;}
.after{font-size:12px; text-align:center; padding:10px; color:red;}
.after a{color:red;}
/*圆桌会议*/
.yz{ width:1000px; margin:0 auto;}
.yz_1{ width:980px; margin:0 auto; margin-top:20px; overflow:hidden;}
#wenzi{ width:455px; float:left;margin-left:16px; height:170px;}
#wenzi p{ font-size:12px; text-align:left; margin:0; padding:0;}
#wenzi_2{ text-indent:24px; line-height:25px; font-size:12px;}

.photo{ float:right; width:450px; height:170px; margin-right:12px;}
.lt{ width:980px; margin:0 auto; overflow:hidden;margin-top:10px;}
.lt_1,.lt_2,.lt_3,.lt_4{ float:left; margin-left:10px; margin-right:10px;}
.lt_1 p,.lt_2, p,.lt_3 p,.lt_1 p{ font-size:12px; text-align:center;}

.jc{ width:1000px; margin:auto;}
.content_1,.content_2,.content_3{width:980px; margin:auto; overflow:hidden; margin-top:10px;}
.jc_1{ float:left; margin-left:10px; margin-right:10px;}
.jc_1 p{ font-size:12px; text-align:center;}
/*往届回顾*/
.container{ width:1000px; height:190px; position:relative; overflow:hidden; margin:auto; margin-bottom:10px;}
.wrapper{ width:1680px;position:absolute; left:0; top:0; }
.wrapper img{ display:block;}
.wrapper a{ float:left; margin:10px;}

/*精彩瞬间*/
.photoBg{ background-image:url(images1/wonderful.png);width:1000px;height:935px;margin: 0 auto; background-position:center center; background-repeat:no-repeat;}
h2{ font-weight:normal; font-size:14px; background-color:#1a60b6; text-align:center; line-height:40px; color:white; width:1000px; margin:auto; margin-bottom:10px;}
.wonderful .box{position: relative;width:915px;height: 642px;margin:0 auto;  padding-top: 110px;
  padding-right: 8px;}
#bigPic{width:846px;height:612px;position:relative;overflow:hidden; margin-left:42px;}
#pic_list{position:relative;z-index:1;}
#pic_list li{
	position: absolute;
	left:5px;
	top:6px;
	opacity:0;
	fliter:alpha(opacity=0);
	z-index:1;
}

#ico_list{
	position: absolute;
	width: 850px;
	overflow: hidden;
	height: 142px;
	z-index: 3;
	left: 22px;
	bottom: -97px;
	margin-left:16px;
}
#ico_list ul{
	width: 4968px;
	position: absolute;
	left: -7px;
	bottom: -23px;
}
#ico_list li{width:206px;height:142px;float:left;padding-left:10px;}
#ico_list li a{width:225px;display:block;}
#ico_list li a img{ height:142px;width:206px;background:#040303;opacity:0.7;filter:alpha(opacity=70);}
#ico_list .active img{opacity:1;filter:alpha(opacity=100);height:142px;width:206px;}

/*会议议程*/
.ba{ background-color:white; width:1000px; margin:auto;}
.list_1{ font-weight:bold; line-height:30px;text-align:center; font-size:14px;}
.list_2{ line-height:30px; text-align:center;font-size:14px;}
.list_3{font-size:14px; line-height:20px;}

.footer_ba{ width:1000px; margin:auto; background-image:url(images/footer_ba.png); background-position:center center; background-repeat:no-repeat;}
.hr{ border-bottom:1px dashed red; width:1000px; margin:auto;padding-top:8px;}
.foot{ width:900px;margin:auto;  height:20px; margin-top:10px;  background-color:white;padding-left:100px;}
.foot a{ font-size:12px}
.foot a:hover{ text-decoration:underline; color:#933}
.footer{ background-color:white; width:1000px; margin:auto; height:20px;}
.footer a{text-align:center; font-size:12px; line-height:20px; display:block;}
